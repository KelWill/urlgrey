{
  "name": "urlgrey",
  "description": "urlgrey is a library for url querying and manipulation",
  "keywords": [
    "url",
    "uri"
  ],
  "version": "0.1.1",
  "bugs": {
    "url": "https://github.com/cainus/urlgrey/issues"
  },
  "scripts": {
    "test": "make test-coveralls"
  },
  "maintainers": [
    "Gregg Caines <gregg@caines.ca> (http://caines.ca)"
  ],
  "dependencies": {
    "qs": "0.6.5",
    "chai": "1.8.1"
  },
  "devDependencies": {
    "jscoverage": "0.3.6",
    "mocha-lcov-reporter": "0.0.1",
    "coveralls": "2.0.4",
    "mocha": "1.8.1",
    "browserify": "2.35.2",
    "uglify-js": "2.4.3"
  },
  "engine": {
    "node": ">=0.8.6"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "browser": {
    "url": "./browser/url.js"
  },
  "testling": {
    "browsers": [
      "ie6", "ie7", "ie8", "ie9",
      "firefox/15", "chrome/22", "opera/12", "safari/5.1"
    ],
    "harness" : "mocha",
    "files": "test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cainus/urlgrey.git"
  },
  "author": "Gregg Caines",
  "license": "BSD-2-Clause"
}
